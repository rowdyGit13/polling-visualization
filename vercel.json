{
    "version": 2,
    "builds": [
      {
        "src": "mysite/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/vercel-test",
        "dest": "mysite/wsgi.py"
      },
      {
        "src": "/(.*)",
        "dest": "mysite/wsgi.py"
      }
    ],
    "env": {
      "VERCEL_DEPLOYMENT": "true",
      "DJANGO_SETTINGS_MODULE": "mysite.settings",
      "DEBUG": "False"
    }
  }
  